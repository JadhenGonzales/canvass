{  
    "manifest_version": 2,
    "name": "Canvass",
    "version": "1.0",

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_title": "Canvass",
        "default_popup": "popup/index.html"
    },

    "permissions": ["activeTab","clipboardWrite", "downloads"]
}